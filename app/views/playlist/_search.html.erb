<% content_for :head do %>
  <title>jukebox.rb &mdash; <%#= now_playing.title %></title>
  <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.3/jquery.min.js" type="text/javascript"></script> 
  <script src="/javascripts/tracks.js" type="text/javascript"></script>
  <script src="/javascripts/search.js" type="text/javascript"></script>
<% end %>

<!-- Search Box -->
<div id="search" >
  <label for="fuzzy">Search</label> <input id="fuzzy" type="text" onChange="fuzzy_change(value)" onKeyUp="fuzzy_change(value, event)"
                                    onSubmit="fuzzy_search(value);" size="50" />
</div>

<!-- Spacer -->
<div align="center">
  <br />
</div>

<!-- Results -->
<div id="results" align="center"></div>
